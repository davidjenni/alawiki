{
  "name": "alawiki",
  "version": "0.0.1",
  "description": "Fast, flat-file wiki in node.js",
  "main": "index.js",
  "engines": {
    "node": "~0.10"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "start": "node index.js",
    "debug": "bin/debug",
    "test": "npm run jshint && npm run mocha",
    "mocha": "mocha test/**/*test.coffee",
    "jshint": "node_modules/.bin/jshint lib/*.js lib/routes/*.js index.js",
    "watch": "catw -c 'node_modules/.bin/jshint lib/*.js index.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidjenni/alawiki"
  },
  "keywords": [
    "wiki",
    "nodejs"
  ],
  "author": "David Jenni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidjenni/alawiki/issues"
  },
  "homepage": "https://github.com/davidjenni/alawiki",
  "dependencies": {
    "marked": "~0.2.10",
    "express": "~3.4.7",
    "jade": "~0.35.0",
    "stylus": "~0.41.3",
    "winston": "~0.7.2"
  },
  "devDependencies": {
    "mocha": "~1.15.1",
    "coffee-script": "~1.6.3",
    "sinon-chai": "~2.4.0",
    "chai": "~1.8.1",
    "sinon": "~1.7.3",
    "jshint": "~2.3.0",
    "supertest": "~0.8.2",
    "catw": "~0.2.0",
    "tmp": "0.0.23"
  }
}
